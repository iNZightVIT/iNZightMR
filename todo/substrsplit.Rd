## The function is currently defined as
function (obj) 
{
    str <- names(obj)
    if (is.vector(obj)) 
        str <- obj
    n <- max(nchar(str))
    i <- 0
    while (length(unique(substr(str, 1, i))) == 1) {
        i <- i + 1
    }
    commonstr <- unique(substr(str, 1, i - 1))
    varname <- substr(str, i, n)
    list(Commonstr = commonstr, Varname = varname)
  }
}
